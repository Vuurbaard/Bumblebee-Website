{
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": [
					"/favicon.ico",
					"/index.html",
          "/manifest.webmanifest"
				],
				"versionedFiles": [
					"/*.bundle.css",
					"/*.bundle.js",
					"/*.chunk.js"
				]
			}
		},
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/assets/**"
				]
			}
		}
	],
	"dataGroups": [
		{
			"name": "api-freshness",
			"urls": [
				"/tts"
			],
			"cacheConfig": {
				"maxSize": 100,
				"maxAge": "1h",
				"timeout": "10s",
				"strategy": "freshness"
			}
		}
	]
}